{
  "version": 2,
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://netnav.app",
    "NEXT_PUBLIC_APP_DOMAIN": "netnav.app"
  },
  "github": {
    "silent": true
  },
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}