# Database
DATABASE_URL="postgresql://username:password@localhost:5432/netnav?schema=public"

# OpenCage Geocoding API
OPENCAGE_API_KEY="your_api_key_here"

# Node Environment
NODE_ENV="development"

# Application
NEXT_PUBLIC_APP_URL="https://netnav.app"
NEXT_PUBLIC_APP_DOMAIN="netnav.app"
NEXT_PUBLIC_APP_NAME="NetNav"

# Developer preview access
# These are example values. In production, use secure, randomly generated values.
DEV_ACCESS_CODE="dev-preview-2024"

# Mailchimp Configuration
MAILCHIMP_API_KEY="your_mailchimp_api_key_here"
MAILCHIMP_LIST_ID="your_mailchimp_list_id_here"
MAILCHIMP_SERVER_PREFIX="usXX"  # The server prefix from your API key (e.g., us15) 